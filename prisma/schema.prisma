datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

model Challenge {
  id String @id @default(uuid())

  title       String
  description String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("challenge")
}
